registers {
- ax : Reg 8
- bx : Reg 8
- cx : Reg 8
- dx : Reg 8
}

<Reg 8> : Bits 2
<ax> = 0b00
<bx> = 0b01
<cx> = 0b10
<dx> = 0b11

instructions {
- add <Reg 8> <Reg 8>
- const <Reg 8> <Int 8>
}

<Inst> : Bits 5
<add <r1> <r2>> = 0b0 ++ <r1> ++ <r2>
<const <r1> <n>> = 0b1 ++ <r1> ++ <n>

add <r1> <r2> {
  r1 <- r1 + r2
}

const <r1> <n> {
  r1 <- n
}

